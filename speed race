}

game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.Lobby.Collecting.CollectedSpeed:FireServer(unpack(args))
                
            until getgenv().nigger == false
        else
            getgenv().nigger = false
        end

    end


end)
